{
  "name": "pratica_backend_1",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "swagger-autogen": "^2.21.5",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.2.34"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.7.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/typescript": "^2.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.0.3",
    "sqlite3": "^5.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "migration:run": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "migration:test": "cross-env NODE_ENV=test && migration:run",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "start": "node ./dist/server",
    "doc": "node ./swagger",
    "test:setup": "rimraf ./db-test.sql && migration:test",
    "test": "cross-env NODE_ENV=test && test:setup && jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "cross-env NODE_ENV=test && test:setup && jest --passWithNoTests --runInBand ----detecOpenHandles",
    "test:coverage": "test --coverage",
    "test:unit": "tests --watchAll -c ./jest-unit.config.ts",
    "test:integration": "tests --watchAll -c ./jest-integration.config.ts"
  }
}
